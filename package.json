{
  "name": "my-commit",
  "private": true,
  "packageManager": "pnpm@9.0.0",
  "scripts": {
    "dev": "turbo run dev",
    "dev:web": "pnpm --filter my-commit-client dev",
    "build": "turbo run build",
    "build:web": "pnpm --filter my-commit-client build",
    "dev:api": "pnpm --filter my-commit-api dev",
    "build:api": "pnpm --filter my-commit-api build",
    "test": "turbo run test",
    "test:web": "pnpm --filter my-commit-client test",
    "lint": "turbo run lint",
    "lint:fix": "turbo run lint:fix",
    "fix:backup-json": "node scripts/fix-contaminated-backup.mjs",
    "deploy": "bash scripts/deploy.sh",
    "deploy:prod": "pnpm run deploy -- prod",
    "deploy:dev": "pnpm run deploy -- dev",
    "deploy:prod:porkbun": "UPDATE_DNS_RECORD=true pnpm run deploy -- prod --dns-provider porkbun",
    "deploy:dev:porkbun": "UPDATE_DNS_RECORD=true pnpm run deploy -- dev --dns-provider porkbun",
    "create:aws:acm": "bash scripts/create-aws-acm.sh",
    "create:aws:acm:prod:porkbun": "pnpm run create:aws:acm -- prod --dns-provider porkbun --wait",
    "create:aws:acm:dev:porkbun": "pnpm run create:aws:acm -- dev --dns-provider porkbun --wait",
    "dns:sync": "bash scripts/sync-dns.sh",
    "dns:sync:porkbun": "pnpm run dns:sync -- --dns-provider porkbun",
    "dns:check:porkbun": "bash scripts/check-porkbun-dns.sh",
    "undeploy": "bash scripts/reset-aws.sh",
    "undeploy:prod": "pnpm run undeploy -- prod --remove-acm",
    "undeploy:dev": "pnpm run undeploy -- dev --remove-acm"
  },
  "devDependencies": {
    "tsx": "^4.7.1",
    "turbo": "^1.10.0"
  },
  "dependencies": {
    "eslint-plugin-react-hooks": "7.1.0-canary-d6cae440-20260106",
    "jose": "^6.1.3"
  }
}
